{
  "name": "techmentorweb",
  "version": "1.0.0",
  "description": "Website for devmentorfor.me",
  "main": "dist/server.js",
  "private": true,
  "config": {
    "compileTarget": "local",
    "configuration": "debug",
    "selfHost": true,
    "port": "8043"
  },
  "scripts": {
    "init": "yarn install",
    "prebuild": "npm run init",
    "build": "node compiler/index.js build",
    "preclean": "npm run init",
    "clean": "node compiler/index.js clean",
    "prerebuild": "npm run init",
    "rebuild": "node compiler/index.js rebuild",
    "pretest": "npm run init",
    "test": "node ./node_modules/karma/bin/karma start",
    "prestart": "npm run init",
    "start": "node compiler/index.js runServer",
    "buildAgent": "set compileTarget=hosted&& node compiler/index.js rebuild",
    "localDebug": "set compileTarget=local&& npm run start",
    "debugBuild": "set selfHost=false&& npm run rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Divergic/techmentorweb.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Divergic/techmentorweb/issues"
  },
  "homepage": "https://github.com/Divergic/techmentorweb#readme",
  "dependencies": {
    "auth0-js": "^8.9.3",
    "axios": "^0.16.2",
    "compression": "^1.7.0",
    "es6-promise": "^4.1.1",
    "helmet": "^3.8.1",
    "helmet-csp": "^2.5.1",
    "izitoast": "^1.1.5",
    "mime-types": "^2.1.16",
    "serve-static": "^1.12.4",
    "store": "^2.0.12",
    "vee-validate": "^2.0.0-rc.14",
    "vue": "^2.4.2",
    "vue-class-component": "^5.0.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^2.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.54",
    "@types/node": "^8.0.25",
    "css-loader": "^0.28.5",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs": "^0.0.1-security",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-verbose-reporter": "^0.0.6",
    "karma-webpack": "^2.0.4",
    "make-runnable": "^1.3.6",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.3",
    "open-browser-webpack-plugin": "^0.0.5",
    "path": "^0.12.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.4",
    "typescript": "^2.5.1",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-vendor-chunk-plugin": "^1.0.0"
  }
}

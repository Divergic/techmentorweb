{
  "name": "techmentorweb",
  "version": "1.0.0",
  "description": "Website for devmentorfor.me",
  "main": "dist/server.js",
  "private": true,
  "subPackages": [
    "server"
  ],
  "scripts": {
    "install": "yarn install --ignore-scripts",
    "postinstall": "npm rebuild node-sass && subpkg install",
    "prebuild": "npm run install",
    "build": "node compiler/index.js build",
    "preclean": "npm run install",
    "clean": "node compiler/index.js clean",
    "prerebuild": "npm run install",
    "rebuild": "node compiler/index.js rebuild",
    "pretest": "npm run install",
    "test": "node ./node_modules/karma/bin/karma start",
    "prestart": "npm run install",
    "start": "node compiler/index.js runServer",
    "buildAgentClean": "set compileTarget=hosted&& node compiler/index.js clean",
    "buildAgentBuild": "set compileTarget=hosted&& node compiler/index.js build",
    "buildAgentTest": "set compileTarget=hosted&& node ./node_modules/karma/bin/karma start",
    "localDebug": "set compileTarget=local&& npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Divergic/techmentorweb.git"
  },
  "author": "roryprimrose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Divergic/techmentorweb/issues"
  },
  "homepage": "https://github.com/Divergic/techmentorweb#readme",
  "dependencies": {
    "auth0-js": "^9.4.2",
    "axios": "^0.18.0",
    "es6-promise": "^4.1.1",
    "izitoast": "^1.3.0",
    "marked": "^0.3.6",
    "raven-js": "^3.24.2",
    "store": "^2.0.12",
    "svg-inline-loader": "^0.8.0",
    "tz-ids": "^1.0.0",
    "vee-validate": "^2.0.0-rc.14",
    "vue": "^2.4.2",
    "vue-application-insights": "^1.0.3",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vue-upload-component": "^2.8.5",
    "vuetify": "^1.0.16",
    "vuex": "^3.0.0",
    "vuex-persistedstate": "^2.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.54",
    "@types/node": "^9.6.6",
    "css-loader": "^0.28.5",
    "file-loader": "^1.1.5",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-verbose-reporter": "^0.0.6",
    "karma-webpack": "^3.0.0",
    "make-runnable": "^1.3.6",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "^4.5.3",
    "open-browser-webpack-plugin": "^0.0.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "subpackage": "^1.1.0",
    "ts-loader": "^4.2.0",
    "tslint": "^5.7.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-vendor-chunk-plugin": "^1.0.0"
  }
}
